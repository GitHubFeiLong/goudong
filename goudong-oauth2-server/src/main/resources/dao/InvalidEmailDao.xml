<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.goudong.oauth2.dao.InvalidEmailDao">
    <sql id="invalid_email_column">
        uuid,
        email,
        is_delete,
        update_time,
        create_time
    </sql>
    <insert id="insert">
        insert into invalid_email (email) values (#{email})
    </insert>
    <select id="selectByEmail" resultType="com.goudong.commons.entity.InvalidEmailDO">
        select <include refid="invalid_email_column"/> from invalid_email where email=#{email} limit 1
    </select>

</mapper>
